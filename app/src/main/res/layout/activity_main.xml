<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewWebviewer"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/textViewGoogle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewGoogle"
        android:textSize="24sp" />

    <Button
        android:id="@+id/buttonGoogle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/buttonGoogle"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textViewUCSDExt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewUCSDExt"
        android:textSize="24sp" />

    <Button
        android:id="@+id/buttonUCSDExt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/buttonUCSDExt"
        android:textSize="24sp" />

</LinearLayout>